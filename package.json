{
  "name": "roundel",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^23.1.5",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-native": "^0.56.0",
    "@types/react-test-renderer": "^16.0.1",
    "concurrently": "^3.6.0",
    "jest-expo": "~27.0.0",
    "react-native-scripts": "1.14.0",
    "react-test-renderer": "16.3.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "jest",
    "lint": "tslint src/**/*.ts",
    "tsc": "tsc",
    "clean": "rimraf build",
    "copyAssets": "mkdir -p ./build/assets && cp -R ./assets ./build",
    "build": "yarn run clean && yarn run copyAssets && yarn run tsc",
    "watch": "yarn run build -w",
    "watchAndRunAndroid": "concurrently \"yarn run watch\" \"yarn run android\"",
    "buildRunAndroid": "yarn run build && yarn run watchAndRunAndroid ",
    "watchAndRunIOS": "concurrently \"yarn run watch\" \"yarn run ios\"",
    "buildRunIOS": "yarn run build && yarn run watchAndRunIOS ",
    "watchAndStart": "concurrently \"yarn run watch\" \"yarn run start\"",
    "buildAndStart": "yarn run build && yarn run watchAndStart",
    "cleanModules": "rm -rf $TMPDIR/metro-cache-* && rm -rf node_modules && yarn clean && yarn"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@types/expo": "^27.0.6",
    "@types/expo__vector-icons": "^6.2.3",
    "@types/react-navigation": "^2.0.12",
    "exp-cli": "^5.0.0",
    "expo": "^28.0.0",
    "global": "^4.3.2",
    "react": "16.3.1",
    "react-art": "^16.4.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-28.0.0.tar.gz",
    "react-native-actionsheet": "^2.4.2",
    "react-navigation": "^2.6.2"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
